{
  "name": "express-scaffold",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "precommit": "npm run lint",
    "lint": "eslint --ext .js src",
    "build": "babel src --out-dir dist",
    "start": "node dist/bin/server.js",
    "dev": "cross-env nodemon src/bin/server.js --exec babel-node"
  },
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "~4.15.5",
    "log4js": "^2.7.0",
    "morgan": "~1.9.0",
    "rotating-file-stream": "^1.3.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.2",
    "husky": "^0.14.3",
    "nodemon": "^1.17.1"
  }
}
